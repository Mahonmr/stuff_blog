<div class="container">
  {{#if model}}
    {{#each model as |blog|}}
      <div class="row well">
        <div class="col-md-1">
          {{#link-to 'show-blog' blog}}<img style="max-width:30px; margin-top: 22px;" src="bike.png">{{/link-to}}
        </div>
        <div class="col-md-6">
          <h2>{{#link-to 'edit-blog' blog}}{{blog.subject}}{{/link-to}}</h2>
        </div>
      </div>
    {{/each}}
  {{else}}
    <h2>Let's add some blogs!</h2>
  {{/if}}
</div>
